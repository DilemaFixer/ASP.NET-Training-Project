@using UdemProject.Data

@model List<Category>

<h1>Category Administration Page</h1>
<table class = "table table-bordered table-striped"> 
    <thead>
        <tr> 
            <th> 
                Category Name
            </th>
            <th>
                Category Order
            </th>
        </tr>
    </thead>
    <tbody> 
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
               <td>@obj.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>